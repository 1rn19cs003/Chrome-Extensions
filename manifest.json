{
    "manifest_version":3,
    "name":"Change backgroud color",
    "description":"Add a new background Color",
    "version":"1.0",
    "background":{
        "service_worker":"background.js"
    },
    "action":{
        "default_popup":"popup.html"
    },
    "permissions":["storage","activeTab","scripting"],
    "chrome_url_overrides":{
        "newtab":"www.google.com/"
    }
}